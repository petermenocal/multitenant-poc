{
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "test-b2c.petermenocal.com"
        }
      ],
      "destination": "/b2cstore/$1"
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "test-b2b.petermenocal.com"
        }
      ],
      "destination": "/b2bstore/$1"
    }
  ]
}